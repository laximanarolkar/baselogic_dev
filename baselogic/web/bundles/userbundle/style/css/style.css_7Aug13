.default_popuptab td {
	padding-bottom:12px;

}
.default_popuptab label {
	width:95px;
	float:left;
	color:#474747;
	padding:10px 0 0 0px;
}
.smallselect {
	border: 1px solid #B1B1B1;
	padding: 11px 10px 11px 10px;
	width: 190px;
}
.zipcodepop {
	width:100%;
	float:left;
}
.zipcodepop h4 {
	color: #1f3a7f;
	font-size:18px;
	padding: 0 0 10px;
}
.zipcodepop h4 a {
	color: #178BFF;
}
.zipcodepop p {
	color: #252525;
	padding: 0 0 10px;
	line-height:21px;
}
.largeinpt {
	border: 1px solid #B1B1B1;
	padding: 12px 3px 12px 10px;
	width:300px;
}
.cartbox {
	width:610px;
	float:left;
	margin-bottom:20px;
}
.cartboxtab {
	width:100%;
	border-collapse:collapse;
}
.cartboxtab td {
	padding:10px;
	color:#747474;
}
.cartboxtab th {
	padding:10px;
	color:#252525;
	font-weight:normal;
	font-size:16px;
}
.firstrow {
	background:#f8faff;
	border:1px solid #d1d7e4;
	border-collapse:collapse;
}
.bluetxt {
	color:#085687;
	font-family: 'droid_sansbold';
}
a.editlink {
	cursor:pointer;
	font-size:13px;
	text-decoration:underline;
	color:#085687;
}
a:hover.editlink {
	text-decoration:none;
}
.collapsemain {
	width:100%;
	float:left;
}
.maincartbox {
	width:100%;
	float:left;
}
.configuration {
	width:615px;
	border:1px solid #dddee0;
	background:#f8faff;
	padding:20px 20px 20px 20px;
	margin:20px 0 0 0px;
}
.configuration h4 {
	color: #5FB761;
	font-family: 'droid_sansbold';
	font-size: 16px;
	padding-bottom: 15px;
}
.collapsemain h3 {
	color:#085687;
	font-size:13px;
