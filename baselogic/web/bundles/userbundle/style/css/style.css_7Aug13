}
.defaultpopup {
	width:100%;
	float:left;
}
.defaultpopup h4 {
	color: #1f3a7f;
	font-size:18px;
	padding: 0 0 10px;
}
.defaultpopup h4 a {
	color: #178BFF;
}
.defaultpopup p {
	color: #252525;
	padding: 0 0 10px;
	line-height:21px;
}
.defaultpopup {
	width:100%;
}
.default_popuptab {
	width:100%;
