<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Windows" table="windows">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="float" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="manufacturer" type="string" column="manufacturer" length="255"/>
    <field name="line" type="string" column="line" length="255"/>
    <field name="frame" type="string" column="frame" length="255"/>
    <field name="width" type="float" column="width"/>
    <field name="height" type="float" column="height"/>
    <field name="callout" type="string" column="callout" length="255"/>
    <field name="ui" type="string" column="ui" length="255"/>
    <field name="base" type="float" column="base"/>
    <field name="lowe" type="float" column="lowe"/>
    <field name="tempered" type="float" column="tempered"/>
    <field name="obscure" type="float" column="obscure"/>
    <field name="flat" type="float" column="flat"/>
    <field name="sculptured" type="float" column="sculptured"/>
    <field name="argon" type="float" column="argon"/>
    <field name="style" type="string" column="style" length="255"/>
    <field name="material" type="string" column="material" length="255"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
